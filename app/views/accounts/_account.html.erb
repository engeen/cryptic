<div class='account'>
  <div class='no'>
    <%= account.id%>
  </div>
  <div class='name'>
    <%= link_to account.name, account_url(account)%>
  </div>
  <div class='role'>
    <%= begin current_user.member?(account).role; rescue Exception => e; "-"; end %>
  </div>
  <div style='clear: both'>&nbsp;</div>
</div>
